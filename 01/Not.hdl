/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Nand(a,a) = Not(a)
    // Table of Truth:
    // Nand(0,0) = 1 = Not(0)
    // Nand(1,1) = 0 = Not(1)
    Nand(a = in, b = in, out = out);
}